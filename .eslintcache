     })}\r\n    </ul>\r\n    </nav>\r\n     )\r\n\r\n}\r\n\r\n\r\n\r\n\r\n\r\nexport default Navbar;","C:\\Users\\user\\Desktop\\React-practice\\manureactapp\\react-tutorial-update\\src\\functionBased\\pages\\SinglePage.js",[],{"ruleId":"57","severity":1,"message":"58","line":11,"column":9,"nodeType":"59","messageId":"60","endLine":11,"endColumn":14},{"ruleId":"57","severity":1,"message":"61","line":11,"column":15,"nodeType":"59","messageId":"60","endLine":11,"endColumn":21},{"ruleId":"57","severity":1,"message":"62","line":14,"column":8,"nodeType":"59","messageId":"60","endLine":14,"endColumn":13},{"ruleId":"57","severity":1,"message":"63","line":15,"column":8,"nodeType":"59","messageId":"60","endLine":15,"endColumn":15},{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","replacedBy":"67"},{"ruleId":"57","severity":1,"message":"68","line":4,"column":10,"nodeType":"59","messageId":"60","endLine":4,"endColumn":21},{"ruleId":"57","severity":1,"message":"58","line":2,"column":9,"nodeType":"59","messageId":"60","endLine":2,"endColumn":14},{"ruleId":"57","severity":1,"message":"61","line":2,"column":22,"nodeType":"59","messageId":"60","endLine":2,"endColumn":28},{"ruleId":"57","severity":1,"message":"69","line":2,"column":30,"nodeType":"59","messageId":"60","endLine":2,"endColumn":43},{"ruleId":"70","severity":1,"message":"71","line":61,"column":10,"nodeType":"72","endLine":61,"endColumn":22},"no-unused-vars","'Route' is defined but never used.","Identifier","unusedVar","'Routes' is defined but never used.","'About' is defined but never used.","'NoMatch' is defined but never used.","no-native-reassign",["73"],"no-negated-in-lhs",["74"],"'IconContext' is defined but never used.","'useRouteMatch' is defined but never used.","jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it wit